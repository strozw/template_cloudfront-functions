{
	"$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "incremental": true,
    "target": "ES5",                                  
    "lib": ["ES5"],
    "module": "CommonJS",
		"moduleResolution": "Node",
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,            
    "strict": true,                                      
    "skipLibCheck": true                                 
  }
}
